<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="ColeccionCoras_PU" transaction-type="JTA">
    <jta-data-source>jdbc/coleccioncoras</jta-data-source>
    <class>sv.coras.entity.Casa</class>
    <class>sv.coras.entity.Coleccion</class>
    <class>sv.coras.entity.Cora</class>
    <class>sv.coras.entity.Departamento</class>
    <class>sv.coras.entity.Estado</class>
    <class>sv.coras.entity.Pais</class>
    <class>sv.coras.entity.Sitio</class>
    <class>sv.coras.entity.Usuario</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties/>
  </persistence-unit>
</persistence>
